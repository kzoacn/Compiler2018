
int f(int x,int y){
   int c=x+y;
   return c;
}

int main(){
    int x=3;
    x++;
    int i=0;
    int y=0;
    for(i=0;i<5;i++){
        x=x+y;
        y=i+1;
        println(toString(i));
        println(toString(x));
        println(toString(y));
        println(toString(f(x,y)));
    }
    return 0;
}
